# n8n Environment Variables
# Copy this file to .env and fill with your values

# n8n Configuration
N8N_HOST=localhost
N8N_PORT=5678
N8N_PROTOCOL=http
N8N_BASIC_AUTH_ACTIVE=true
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=change_this_password

# Webhook URL
N8N_WEBHOOK_URL=http://localhost:5678/webhook

# Chatwoot Integration
CHATWOOT_URL=http://localhost:3000
CHATWOOT_API_TOKEN=your_chatwoot_api_token_here
CHATWOOT_ACCOUNT_ID=3

# Database
N8N_DB_TYPE=postgresdb
N8N_DB_POSTGRESDB_HOST=tiktrend-postgres
N8N_DB_POSTGRESDB_PORT=5432
N8N_DB_POSTGRESDB_DATABASE=tiktrend
N8N_DB_POSTGRESDB_USER=tiktrend
N8N_DB_POSTGRESDB_PASSWORD=your_postgres_password

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0

# OpenAI
OPENAI_API_KEY=your_openai_api_key_here

# Google Calendar (optional)
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=
GOOGLE_REFRESH_TOKEN=

# Rate Limiting
RATE_LIMIT_MESSAGES_PER_MINUTE=10
RATE_LIMIT_WINDOW_SECONDS=60

# Logging
LOG_LEVEL=info
LOG_TO_FILE=true
LOG_FILE_PATH=/var/log/n8n/workflow.log

# Evolution API
EVOLUTION_API_URL=http://localhost:8082
EVOLUTION_API_KEY=your_evolution_api_key

# Feature Flags
ENABLE_AI_FALLBACK=true
ENABLE_RATE_LIMITING=true
ENABLE_CONTEXT_TRACKING=true
ENABLE_AB_TESTING=false
